@model IEnumerable<Practica8Unidad2.Models.Inscripcion>

@{
    ViewBag.Title = "Index";
}

<h2>Mantenimiento de Inscripciones</h2>

<p>
    @Html.ActionLink("Nueva Inscripcion", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CursoId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstudianteId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semestre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CursoId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstudianteId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Semestre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
